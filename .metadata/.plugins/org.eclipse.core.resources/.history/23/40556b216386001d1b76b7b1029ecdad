		public class Node 
			{
				int value;
				Node left;
				Node right;

				Node(int value) 
				{
					this.value = value;
					right = null;
					left = null;
				}
			}
		public class BinaryTree 
		{
			Node root;
			
			public void add(int value)
			{
				BinaryTree bt = new BinaryTree();
				bt.add(50);
				bt.add(30);
				bt.add(45);
				bt.add(12);
				bt.add(29);
	           
	            System.out.print("The contents of the binary tree are: " + bt);
	        
			}
			
			
